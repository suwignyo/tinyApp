<header>
      <% if(user){ %>
      <%= user.email%>
      <form action="/logout" method="POST" style="margin:150px">
      <input type="submit" value="Logout">
       </form>
      <% } else { %>
      <td colSpan="2">
      <a href="/login">Login</a>
      <a href="/register">Register</a>
      </td>
      <% } %>
  </header>
